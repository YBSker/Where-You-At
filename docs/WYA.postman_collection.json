			"name": "Update Places",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "a30460fe-1c21-4a60-9a10-3231490e3ae4",
						"exec": [
							"pm.sendRequest({ url: \"http://localhost:7000/places\", method: \"POST\" }, () => {",
							"    pm.sendRequest(\"http://localhost:7000/places\", (err, response) => {",
							"        pm.environment.set(\"placesIdentifier\", response.json().last().identifier);",
							"        pm.sendRequest({",
							"            url: 'http://localhost:7000/places/' + pm.environment.get(\"placesIdentifier\"),",
							"            method: 'POST',",
							"            body: {",
							"              mode: 'formdata',",
							"              formdata: [",
							"                {key: \"town\", value: \"create event\"},",
							"                {key: \"city\", value: \"some event\"},",
							"                {key: \"state\", value: \"Johns Hopkins University\"},",
							"                {key: \"country\", value: \"help\"}",
							"              ]",
							"            }",
							"        });",
							"    });",
							"});"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "ea6ab520-babe-4149-90d3-c65a807a90a3",
						"exec": [
							"pm.test(\"Status code is 204\", () => {",
							"    pm.response.to.have.status(204);",
							"});",
							"",
							"pm.test(\"Item is edited\", () => {",
							"    pm.sendRequest(\"http://localhost:7000/places\", (err, response) => {",
							"        pm.expect(response.json().first().town).to.equal(\"Hampden\");",
							"        pm.expect(response.json().first().city).to.equal(\"Baltimore\");",
							"        pm.expect(response.json().first().state).to.equal(\"Maryland\");",
							"        pm.expect(response.json().first().country).to.equal(\"USA\");",
							"    });",
							"});",
							"",
							"pm.test(\"Status code is 404 if item isn’t found\", () => {",
							"    pm.sendRequest({ url: \"http://localhost:7000/places/51039\", method: \"PUT\" }, (err, response) => {",
							"        pm.expect(response).to.have.status(404);",
							"    });",
							"});"
						],
